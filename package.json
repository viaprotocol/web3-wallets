{
  "name": "@viaprotocol/web3-wallets",
  "type": "module",
  "version": "1.0.31-rc.2",
  "private": false,
  "description": "Universal interface for web3 wallets",
  "author": "Via Protocol (https://via.exchange)",
  "license": "MIT",
  "repository": "https://github.com/viaprotocol/web3-wallets.git",
  "bugs": {
    "url": "https://github.com/viaprotocol/web3-wallets/issues"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "react",
    "solana",
    "web3",
    "typescript"
  ],
  "sideEffects": false,
  "source": "src/index.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "module": "./build/index.esm.js",
      "default": "./build/index.cjs.js"
    },
    "./networks": {
      "module": "./build/networks.esm.js",
      "default": "./build/networks.cjs.js"
    },
    "./components": {
      "module": "./build/components/index.esm.js",
      "default": "./build/components/index.cjs.js"
    },
    "./hooks": {
      "module": "./build/hooks/index.esm.js",
      "default": "./build/hooks/index.cjs.js"
    },
    "./utils/evm": {
      "module": "./build/utils/evm.esm.js",
      "default": "./build/utils/evm.cjs.js"
    },
    "./utils/solana": {
      "module": "./build/utils/solana.esm.js",
      "default": "./build/utils/solana.cjs.js"
    },
    "./utils/wallet": {
      "module": "./build/utils/wallet.esm.js",
      "default": "./build/utils/wallet.cjs.js"
    },
    "./utils/cosmos": {
      "module": "./build/utils/cosmos.esm.js",
      "default": "./build/utils/cosmos.cjs.js"
    },
    "./utils/btc": {
      "module": "./build/utils/btc.esm.js",
      "default": "./build/utils/btc.cjs.js"
    },
    "./utils/common": {
      "module": "./build/utils/common.esm.js",
      "default": "./build/utils/common.cjs.js"
    }
  },
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "types": "build/index.cjs.d.ts",
  "files": [
    "/build"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "scripts": {
    "clean": "shx rm -rf ./build",
    "build": "yarn clean && microbundle --visualize --target=node --tsconfig tsconfig.json --format esm,cjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "package": "shx cp -R package.json ./build && shx cp -R README.md ./build",
    "publish:pre": "yarn build && yarn package",
    "publish:yalc": "yarn publish:pre && yalc publish --no-script --push",
    "publish:rc": "npm version prerelease --preid=rc && yarn publish:pre && npm publish --tag rc --access public",
    "publish:latest": "yarn publish:pre && npm publish --tag latest --access public",
    "build:test": "microbundle --visualise --tsconfig tsconfig.json"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "contributors": [
    {
      "name": "Aleksey Lisun",
      "url": "https://github.com/insulineru"
    },
    {
      "name": "Iower",
      "url": "https://github.com/iower"
    },
    {
      "name": "Max Korolev",
      "url": "https://github.com/nrjshka"
    }
  ],
  "peerDependencies": {
    "ethers": "^5.6.8",
    "react": "^18.1.0"
  },
  "dependencies": {
    "@coinbase/wallet-sdk": "^3.3.0",
    "@cosmjs/stargate": "^0.27.0",
    "@gnosis.pm/safe-apps-provider": "^0.13.1",
    "@gnosis.pm/safe-apps-sdk": "^7.8.0",
    "@keplr-wallet/cosmos": "^0.9.9",
    "@keplr-wallet/types": "^0.10.13",
    "@metamask/providers": "^8.1.1",
    "@solana/spl-name-service": "^0.1.4",
    "@solana/wallet-adapter-base": "^0.9.9",
    "@solana/wallet-adapter-phantom": "^0.9.8",
    "@solana/web3.js": "^1.43.5",
    "@tanstack/react-query": "^4.1.3",
    "@walletconnect/web3-provider": "^1.7.8",
    "ismobilejs": "^1.1.1",
    "long": "^5.2.0",
    "rango-sdk": "^0.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.12",
    "eslint": "^8.18.0",
    "microbundle": "^0.15.1",
    "rollup-plugin-polyfill-node": "^0.10.2",
    "shx": "^0.3.4",
    "ts-patch": "^2.0.1",
    "typescript": "^4.7.3",
    "typescript-transform-paths": "^3.3.1"
  }
}
